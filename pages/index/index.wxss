
/* pages/index/index.wxss */
page{
  height: 100%;
}
.indexWrap{
  height: 100%;
  background: #fff;
}
.indexWrap .authMask{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0.4);
  z-index: 999
}
.indexWrap .homeTitle{
  text-align: center;
  font-size: 36rpx;
  padding: 20rpx 0;
	font-family: PingFangSC-Regular;
}
.indexWrap .chooseOneWrap{
  text-align: center;
  padding: 40rpx 0;
}
.indexWrap .chooseOneWrap .chooseOne{
  width: 228rpx;
  height: 315rpx;
}
.indexWrap .chooseTwoWrap{
  text-align: center;
  padding: 70rpx 0 40rpx;
}
.indexWrap .chooseTwoWrap .chooseTwo{
  width: 264rpx;
  height: 357rpx;
}
.indexWrap .homeBtn{
  width: 200rpx;
  height: 68rpx;
  line-height: 68rpx;
  background-color: #4c85fb;
	box-shadow: 0px 4px 13px 0px rgba(26, 73, 210, 0.34);
	border-radius: 4px;
  margin:  auto;
	font-family: PingFangSC-Regular;
	font-size: 28rpx;
  color: #fff;
  text-align: center;
}